<?xml version="1.0" encoding="utf-8" ?>
<PDL>
  <packet name="RES_BROADCAST_ENTER_GAME">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="REQ_LEAVE_GAME">
  </packet>
  <packet name="RES_BROADCAST_LEAVE_GAME">
	<int name="playerId"/>
  </packet>
  <packet name="RES_PLAYER_LIST">
	<list name="player">
	  <bool name="isSelf"/>
	  <int name="playerId"/>
	</list>
  </packet>
  <packet name="REQ_MOVE">
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="RES_MOVE">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
</PDL>